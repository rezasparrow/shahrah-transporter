{
  "AppSettings": {
    "Name": "Transporter"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Quartz": "Server=mssql;Database=shahrah-quartz-stage;User Id=sa;Password=Aa12345678;"
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Http"
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "Path": "logs/log.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Http",
        "Args": {
          "requestUri": "http://seq:8080"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ],
    "Properties": {
      "Application": "Transporter-worker",
      "Environment": "stage"
    }
  }
}